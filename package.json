{
  "name": "react-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --cache --check .",
    "lint": "yarn lint:eslint && yarn lint:prettier",
